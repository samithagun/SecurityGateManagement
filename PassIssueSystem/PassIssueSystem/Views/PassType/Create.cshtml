@model PassIssueSystem.Models.PassType

@{
    ViewBag.Title = "Create Pass Type";
}

<html>
<head>
    <title>Create Pass Type</title>
</head>
<body class='contrast-red'>
    <div id="content" class="col-lg-10 col-sm-11" style="">
        <div class="row">
            <div class="col-lg-12">
                <div class="box">
                    <div class="box-header" data-original-title="">
                        <h2>
                            <i class="icon-user"></i>
                            <span class="break"></span>
                            Create Vehicle Type
                        </h2>
                    </div>
                    <div class="box-content">
                        <form class="form form-horizontal" style="margin-bottom: 0;" method="post" action="#" accept-charset="UTF-8">
                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary(true)

                                <fieldset>
                                    <legend class='box-header blue-background'>Pass Type Details</legend>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.PassCode)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.PassCode, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.PassCode)
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.Description)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.Description, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.Description)
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.PassFee)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.PassFee, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.PassFee)
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.Status)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.Status, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.Status)
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.AddDate)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.AddDate, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.AddDate)
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-2 control-label">
                                            @Html.LabelFor(model => model.AddUser)
                                        </div>
                                        <div class="col-md-5">
                                            @Html.TextBoxFor(model => model.AddUser, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.AddUser)
                                        </div>
                                    </div>

                                    <div class='form-actions form-actions-padding-sm'>
                                        <div class='row'>
                                            <div class='col-md-10 col-md-offset-2'>
                                                <button class='btn btn-primary' type='submit' value="Create">
                                                    <i class='icon-save'></i>
                                                    Save
                                                </button>
                                                <button class='btn' type='submit'>Cancel</button>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            }
                        </form>

                        <div>
                            @Html.ActionLink("Back to List", "Index")
                        </div>

                        @section Scripts {
                            @Scripts.Render("~/bundles/jqueryval")
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
