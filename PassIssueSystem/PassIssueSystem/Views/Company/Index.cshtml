@model IEnumerable<PassIssueSystem.Models.Company>

@{
    ViewBag.Title = "Index";
}

<html>
<head>
    <title>Vehicle Type Details</title>
</head>
<body class='contrast-red'>
    <header></header>
    <section id='content'>
        <div class='row' id='content-wrapper'>
            <div class='col-sm-12'>
                <div class='row'>
                    <div class='col-sm-12'>
                        <div class='box bordered-box blue-border'>
                            <p>
                                @Html.ActionLink("Create New", "Create")
                            </p>
                            <table>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CompanyName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Address1)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Address2)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Address3)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.BOIApproveNo)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.BOIApproveDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Telephone1)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Telephone2)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Fax)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Status)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.AddDate)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.AddUser)
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CompanyName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Address1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Address2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Address3)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.BOIApproveNo)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.BOIApproveDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Telephone1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Telephone2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Fax)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Status)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.AddDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.AddUser)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.CompanyID }) |
            @Html.ActionLink("Details", "Details", new { id = item.CompanyID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.CompanyID })
                                        </td>
                                    </tr>
                                }

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>
</html>
